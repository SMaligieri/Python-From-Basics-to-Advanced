# Exercício 1
num = int(input("Número: "))
print(num)

# Exercício 2
num = float(input("Número: "))
print(num)

#Exercício 3
num1 = int(input("Digite o primeiro número: "))
num2 = int(input("Digite o segundo número: "))
num3 = int(input("Digite o terceiro número: "))

soma = num1 + num2 + num3
print("Soma: ", soma)

# Exercício 4
num = float(input("Número: "))
print("Quadrado do número: ", num**2)

#Exercício 5
num = float(input("Número: "))
print(num/5)





